{{#if release.enabled}}
{
  "non-interactive": true,
  "preReleaseId": "next",
  "git": {
    "commitMessage": "chore: %s",
    "tagAnnotation": "%s",
    "requireUpstream": false,
    "requireCleanWorkingDir": false
  },
  "npm": {
    "publish": false
  },
  "github": {
    "release": true,
    "releaseName": "%s",
    "tokenRef": "GITHUB_TOKEN",
    "draft": false
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular"
    }
  }
}
{{/if}}
